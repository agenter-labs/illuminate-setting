{
    "name": "agenter-labs/illuminate-setting",
    "description": "Persistent settings package for Laravel",
    "keywords": [
        "laravel",
        "persistent",
        "settings",
        "config"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Abdulla",
            "email": "dev@agenter.com",
            "homepage": "https://agenter.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3|^8.0",
        "illuminate/support": "^8.0",
        "illuminate/cache": "^8.0",
        "illuminate/database": "^8.0",
        "illuminate/contracts": "^8.0",
        "illuminate/filesystem": "^8.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=4.8",
        "mockery/mockery": "0.9.*",
        "laravel/lumen-framework": "^8.0"
    },
    "autoload": {
    "psr-4": {
            "AgenterLab\\Setting\\": "./src"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "AgenterLab\\Setting\\Provider"
            ],
            "aliases": {
                "Setting": "AgenterLab\\Setting\\Facade"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
